{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"],
    "max-classes-per-file": ["error", 10],
    "max-len": ["error", 140],
    "no-unused-vars": ["off"]
  }
}
